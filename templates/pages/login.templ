package pages

import lagouts "github.com/nikhil-thorat/goauth/templates/layouts"

templ Login() {
	@lagouts.Base() {
		<div>
			<h1>Login</h1>
			<form>
				<input type="text" name="email" placeholder="Email"/>
				<input type="password" name="password" placeholder="Password"/>
				<button type="submit">Login</button>
			</form>
		</div>
	}
}
